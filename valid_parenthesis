unordered_map<char,int>symbols={{'[',-1},{'{',-2},{'(',-3},{']',1},{'}',2},{')',3}};

class Solution {
public:
    bool isValid(string s) {
        stack<char>st;
        for(char bracket:s){
            if(symbols[bracket]<0)
                st.push(bracket);
            else{
                if(st.empty())
                {return false;
                 break;}
                char top=st.top();
                st.pop();
                if(symbols[top]+symbols[bracket]!=0)
                {  return false;
                    break; }
            }
        }
        if(st.empty())
            return true;
        return false;
    }
};
